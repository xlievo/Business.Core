<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Business.Core</AssemblyName>
    <RootNamespace>Business</RootNamespace>
    <Version>1.5.8.2</Version>
    <Description>https://github.com/xlievo/Business.Core
https://www.nuget.org/packages/Business.Core/</Description>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/xlievo/Business.Core</PackageProjectUrl>
    <Copyright>Copyright ©  2014~2020 Business Project - http://www.tmopen.com</Copyright>
    <PackageIconUrl></PackageIconUrl>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <AssemblyVersion>1.5.8.2</AssemblyVersion>
    <FileVersion>1.5.8.2</FileVersion>
    <LangVersion>7.3</LangVersion>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>icon.png</PackageIcon>
    <PackageTags>Business.Core</PackageTags>
    <RepositoryUrl>https://github.com/xlievo/Business.Core</RepositoryUrl>
    <PackageReleaseNotes>This is a C # server - side AOP framework, through annotations to the service of your business</PackageReleaseNotes>
    <Authors>xlievo</Authors>
    <Company>www.tmopen.com</Company>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;Standard</DefineConstants>
    <DocumentationFile>bin\Release\netstandard2.0\Business.Core.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard2.0\Business.Core.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\..\Business.Core\Auth\Annotations.cs" Link="Auth\Annotations.cs" />
    <Compile Include="..\..\Business.Core\Auth\Interceptor.cs" Link="Auth\Interceptor.cs" />
    <Compile Include="..\..\Business.Core\Auth\Token.cs" Link="Auth\Token.cs" />
    <Compile Include="..\..\Business.Core\Business\Arg.cs" Link="Business\Arg.cs" />
    <Compile Include="..\..\Business.Core\Business\Bind.cs" Link="Business\Bind.cs" />
    <Compile Include="..\..\Business.Core\Business\Bootstrap.cs" Link="Business\Bootstrap.cs" />
    <Compile Include="..\..\Business.Core\Business\Doc.cs" Link="Business\Doc.cs" />
    <Compile Include="..\..\Business.Core\Business\IBusiness.cs" Link="Business\IBusiness.cs" />
    <Compile Include="..\..\Business.Core\Business\Logger.cs" Link="Business\Logger.cs" />
    <Compile Include="..\..\Business.Core\Configer\Configer.cs" Link="Configer\Configer.cs" />
    <Compile Include="..\..\Business.Core\Result\IResult.cs" Link="Result\IResult.cs" />
    <Compile Include="..\..\Business.Core\Result\ResultObject.cs" Link="Result\ResultObject.cs" />
    <Compile Include="..\..\Business.Core\Utils\Emit.cs" Link="Utils\Emit.cs" />
    <Compile Include="..\..\Business.Core\Utils\Help.cs" Link="Utils\Help.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Castle.Core" Version="4.4.1" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="System.Reflection.Emit.Lightweight" Version="4.7.0" />
    <PackageReference Include="System.Text.Json" Version="4.7.2" />
    <PackageReference Include="System.Threading.Tasks.Extensions" Version="4.5.4" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Result\" />
    <Folder Include="Configer\" />
    <Folder Include="Business\" />
    <Folder Include="Auth\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\.editorconfig" Link=".editorconfig" />
    <None Include="..\..\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>